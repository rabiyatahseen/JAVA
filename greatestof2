/*greatest of two numbers in java */
public class greatestof2{
    public static void main(String[]args){
        int num1=36, num2=25;
        if(num1 == num2)
             System.out.println("both are equal");
        else if(num1>num2)
            System.out.println(num1 + "is greater");
        else
            System.out.println(num2 + "is greater");
    }
}
